{
  "name": "feedseries",
  "desktopName": "FeedSeries",
  "productName": "FeedSeries",
  "version": "1.3.3",
  "author": {
    "name": "Matthieu YK",
    "email": "matthieuy+git@riseup.net"
  },
  "description": "Suivez votre actualité sériephile",
  "license": "MIT",
  "homepage": "https://github.com/matthieuy/feedseries",
  "repository": "https://github.com/matthieuy/feedseries",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && cross-env DEBUG=electron-builder electron-builder -lw",
    "build:linux": "node .electron-vue/build.js && electron-builder --linux appImage",
    "build:win": "node .electron-vue/build.js && electron-builder --win",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "release": "node .electron-vue/build.js && electron-builder -lw --publish always",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "FeedSeries",
    "appId": "com.matthieuy.feedseries",
    "asar": {
      "smartUnpack": true
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "win": {
      "icon": "build/icons/icon-w.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons",
      "category": "AudioVideo",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "generateUpdatesFilesForAllChannels": true,
    "publish": [
      {
        "provider": "github",
        "owner": "matthieuy",
        "vPrefixedTagName": true
      }
    ]
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.18.1",
    "camo": "^0.12.3",
    "canvasjs": "github:matthieuy/canvasjs#master",
    "electron-log": "^2.2.17",
    "electron-store": "^2.0.0",
    "electron-updater": "^4.0.6",
    "fullcalendar": "^3.9.0",
    "jquery": "^3.5.0",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "nedb": "^1.8.0",
    "request": "^2.88.0",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.7",
    "vue-context-menu": "^2.0.6",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-hot-middleware": "^2.18.2"
  }
}
